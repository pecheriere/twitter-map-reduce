<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/tweetFetching</url>
        <description>Fetch new tweets every hours</description>
        <schedule>every 20 minutes</schedule>
    </cron>
    
    <cron>
        <url>/wordcount</url>
        <description>Launch wordCount mapReduce</description>
        <schedule>every 2 hours</schedule>
    </cron>

    <cron>
        <url>/popular</url>
        <description>Launch popular mapReduce</description>
        <schedule>every 2 hours</schedule>
    </cron>

    <cron>
        <url>/associated</url>
        <description>Launch associated mapReduce</description>
        <schedule>every 2 hours</schedule>
    </cron>
</cronentries>
